<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @*@Styles.Render("~/Content/css")*@
    @*@Scripts.Render("~/bundles/modernizr")*@
    <link href="@Url.Content("~/Content/themes/smart/css/bootstrap.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/themes/smart/css/font-awesome.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/themes/smart/css/smartadmin-production.min.css")" rel="stylesheet" />
    @RenderSection("style", false)
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Soft", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Inicio", "Index", "Home", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("Citas", "Index", "Quotes", new { area = "" }, null)</li>
                    <li>@Html.ActionLink("API", "Index", "Help", new { area = "" }, null)</li>
                </ul>
            </div>
        </div>
    </div>
    <div id="content">
        @RenderBody()
    </div>
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.2/jquery.min.js"></script>
    <script>
	    if (!window.jQuery) {
		    document.write('<script src="@Url.Content("~/Content/themes/smart/js/libs/jquery-2.0.2.min.js")" ><\/script>');
        }
    </script>
    <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>
    <script>
		if (!window.jQuery.ui) {
			document.write('<script src="@Url.Content("~/Content/themes/smart/js/libs/jquery-ui-1.10.3.min.js")"><\/script>');
		}
    </script>
    @*@Scripts.Render("~/bundles/jquery")*@
    @*<script>
        if (!window.jQuery.ui) {
            document.write('<script src="@Url.Content("~/Scripts/jquery-ui-1.10.4.blitzer.min.js")"><\/script>');
        }
    </script>*@
    @*@Scripts.Render("~/bundles/bootstrap")*@
    <script src="@Url.Content("~/Content/themes/smart/js/bootstrap/bootstrap.min.js")"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
